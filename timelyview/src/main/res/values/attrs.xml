<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TimelyView">
        <attr name="text_color" />
        <attr name="rounded_corner"/>
        <attr name="stroke_width"/>
    </declare-styleable>

    <declare-styleable name="TimelyTimeView">
        <attr name="text_color"/>
        <attr name="rounded_corner"/>
        <attr name="seperatorsTextSize" />
        <attr name="animation_type"/>
        <attr name="stroke_width"/>
    </declare-styleable>

    <attr name="text_color" format="color"/>
    <attr name="rounded_corner" format="boolean"/>
    <attr name="seperatorsTextSize" format="integer"/>
    <attr name="stroke_width" format="float"/>
    <attr name="animation_type">
        <flag name="zoom" value="1"/>
        <flag name="material" value="2"/>
    </attr>
</resources>